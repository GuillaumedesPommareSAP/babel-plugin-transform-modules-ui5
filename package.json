{
  "name": "babel-plugin-transform-modules-ui5",
  "version": "6.18.1",
  "description": "An unofficial babel plugin for SAP UI5.",
  "main": "dist/index.js",
  "engines": {
    "node": ">=6"
  },
  "keywords": [
    "ui5",
    "sap",
    "sapui5",
    "openui5",
    "babel",
    "babeljs",
    "babel-plugin",
    "babelplugin",
    "es6",
    "module",
    "import",
    "export",
    "typescript",
    "ts"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "babel lib -d dist",
    "build:watch": "babel lib -d dist -w",
    "build:test": "babel --plugins source-map-support lib -d dist",
    "lint": "eslint lib test",
    "lint:fix": "npm run lint -- --fix",
    "test": "npm run build:test & jest test",
    "test:dev": "npm run test -- --watch",
    "prepublish": "npm run clean & npm run build & jest test",
    "precommit": "lint-staged && yarn test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/r-murphy/babel-plugin-ui5.git"
  },
  "author": "Ryan Murphy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/r-murphy/babel-plugin-ui5/issues"
  },
  "homepage": "https://github.com/r-murphy/babel-plugin-ui5#readme",
  "jest": {
    "collectCoverage": true
  },
  "lint-staged": {
    "lib/**/*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "array-flatten": "^2.1.1",
    "babel-jest": "^22.2.2",
    "babel-template": "^7.0.0-beta.3",
    "babel-types": "^7.0.0-beta.3",
    "doctrine": "^2.0.0",
    "ignore-case": "^0.1.0",
    "object-assign-defined": "^1.0.2"
  },
  "devDependencies": {
    "babel-core": "^7.0.0-beta.3",
    "babel-eslint": "^8.0.1",
    "babel-plugin-source-map-support": "^1.0.0",
    "babel-plugin-syntax-class-properties": "^7.0.0-beta.3",
    "babel-plugin-syntax-decorators": "^7.0.0-beta.3",
    "babel-plugin-syntax-dynamic-import": "^7.0.0-beta.3",
    "babel-plugin-syntax-object-rest-spread": "^7.0.0-beta.3",
    "babel-plugin-transform-es2015-duplicate-keys": "^7.0.0-beta.3",
    "babel-plugin-transform-flow-strip-types": "^7.0.0-beta.3",
    "babel-plugin-transform-object-rest-spread": "^7.0.0-beta.3",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.8.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^6.0.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "fs-extra": "^5.0.0",
    "husky": "^0.14.3",
    "jest": "^22.3.0",
    "lint-staged": "^6.0.0",
    "rimraf": "^2.6.2",
    "source-map-support": "^0.5.0"
  }
}
